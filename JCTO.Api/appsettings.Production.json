{
  "Serilog": {
    "Using": [
      "AWS.Logger.SeriLog"
    ],
    "MinimumLevel": "Information",
    "LogGroup": "JCT",
    "Region": "ap-south-1",
    "WriteTo": [
      {
        "Name": "AWSSeriLog",
        "Args": {
          "textFormatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
        }
      }
    ]
  }
}
